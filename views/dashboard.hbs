<div class="form-group">
  <a class="btn btn-primary" href="/profile">To My Profile</a>
</div>

<div class="form-group">
  <a class="btn btn-primary" href="/auction/new">Create New Auction</a>
</div>

<form method="get" action="dashboard" >
  <div class="form-group">
    <select class="custom-select" name="filter">
      <option selected>Filter</option>
      <option value="0">All</option>
      <option value="1">Open</option>
      <option value="-1">Closed</option>
    </select>
    </div>
  <submit>
    <button class="btn btn-danger">Apply</button>
  </submit>
</form>


<table id="table_id">
  <thead>
    <tr>
  	  <th>Pokemon Name</th>
      <th>Picture</th>
  	  <th>Start</th>
      <th>Highest Bid</th>
      <th>Description</th>
  </thead>
  <tbody>
    {{#each auctions}}
    <tr>
  	  <td>{{name}}</td>
  	  <td><img src="{{picture}}"></td>
      <td>
        <div class="row-element">{{start_date}}</div>
      </td>
      <td>
        <div class="row-element">{{highestbid}} Poke Dollars</div>
      </td>
      <td>
        <div class="row-element">{{description}}</div>
      </td>
  	</tr>
  	{{/each}}
  </tbody>
</table>
