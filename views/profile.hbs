<h1>Profile</h1>
<a href="/dashboard">Back to dashboard</a>
<form method="POST" style="max-width: 300px; margin: auto; margin-top: 50px; border: 0.5px solid gray; padding: 30px 30px 30px 30px">
  <div class="form-group">
    <label>Email</label>
    <input type="text" name="email" class="form-control" value="{{userInfo.email}}">
  </div>
  <div class="form-group">
    <label>Phone #</label>
    <input type="text" name="phone" class="form-control" value="{{userInfo.phone}}">
  </div>
  <div class="form-group">
    <label>Street Address</label>
    <input type="text" name="street" class="form-control" value="{{userInfo.street}}">
  </div>
  <div class="form-group">
    <label>City</label>
    <input type="text" name="city" class="form-control" value="{{userInfo.city}}">
  </div>
  <div class="form-group">
    <label>State/Province</label>
    <input type="text" name="state" class="form-control" value="{{userInfo.state}}">
  </div>
  <div class="form-group">
    <label>Country</label>
    <input type="text" name="country" class="form-control" value="{{userInfo.country}}">
  </div>
  <div class="form-group">
    <label>Postal Code</label>
    <input type="text" name="postal" class="form-control" value="{{userInfo.postal}}">
  </div>

  <div class="form-group">
    <button class="btn btn-success">Submit</button>
  </div>
</form>
